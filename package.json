{
  "type": "module",
  "name": "object-recipes",
  "version": "0.0.4",
  "description": "A tiny javascript/typescript library for modifying large deeply-nested objects in a immutable, type-safe and composable manner.",
  "license": "MIT",
  "repository": "github:royhansen99/object-recipes",
  "bugs": "https://github.com/royhansen99/object-recipes/issues",
  "keywords": [
    "nested objects",
    "object recipes",
    "composable recipes"
  ],
  "authors": [
    "Roy Hansen <roy@royhansen.no> (https://github.com/royhansen99)"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "prettier": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "tsc": "tsc  --noEmit",
    "test": "TEST_ENV=true vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.12.0",
    "jotai": "^2.15.0",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4",
    "vitest": "^2.1.3",
    "zustand": "^5.0.8"
  }
}
